{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"singleAttributePerLine": true,
	"htmlWhitespaceSensitivity": "ignore",
	"proseWrap": "preserve",
	"insertPragma": false,
	"printWidth": 120,
	"requirePragma": false,
	"tabWidth": 4,
	"useTabs": true,
	"embeddedLanguageFormatting": "auto"
}
